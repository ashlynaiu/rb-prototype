<script>
$(document).ready( function(){
	// see if there are existing items
	if (!localStorage.getItem("navToggle")) {
		// Set data
		localStorage.setItem("navToggle", 'Open')
	}
	$(document).on('click', '.banana-pants', function (){
    	$('#test-this').append('<p class="banana-pants" style="color:#fff">Suprise<p>');
	});

    $('#menu-btn-temp').click( function() {
        var toggleWidth = $(".tempSideNav").width() == 240 ? "56px" : "240px";
        $('.tempSideNav').animate({ width: toggleWidth });
        var navOpen = $(".tempSideNav").width() == 240 ? "Closed" : "Open";

        localStorage.setItem("navToggle", navOpen);
    });

    var navToggle = localStorage.getItem('navToggle') == 'Closed' ? $('.tempSideNav').width('56') : $('.tempSideNav').width('240');

    $('#clear-storage').click( function() {localStorage.clear(); });
});
</script>
